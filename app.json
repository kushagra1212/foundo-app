{
  "expo": {
    "name": "foundo",
    "slug": "foundo",
    "version": "1.0.3",
    "scheme": "foundo",
    "orientation": "portrait",
    "icon": "src/assets/icons/foundo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/images/splash/android/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kushagra711.foundoapp"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "src/assets/icons/foundo.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.foundo.io",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.kushagra711.foundoapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": ""
    },
    "extra": {
      "eas": {
        "projectId": "14dc1de5-6c6a-4163-8488-b4977b56f03e"
      }
    },
    "owner": "kushagra711",
    "plugins": ["@config-plugins/detox"]
  }
}
